model Course {
	id               Int       @id @default(autoincrement())
	WLK_COURS_FLAG_NM String
	WLK_COURS_NM      String
	COURS_DC          String
	SIGNGU_NM         String
	COURS_LEVEL_NM    String
	COURS_LT_CN       Float
	COURS_DETAIL_LT_CN String
